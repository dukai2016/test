

    {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "definitions": {},
        "id": "http://example.com/example.json",
        "properties": {
            "checked": {
                "id": "/properties/checked",
                "type": "boolean"
            },
            "date": {
                "id": "/properties/date",
                "type": "string",
                "format":"date",
                "pattern": "^(\\d{4})-(0?[1-9]|1[012])-(0?[1-9]|[12][0-9]|3‌​[01])$"
            },
            "id": {
                "id": "/properties/id",
                "type": "integer",
                "pattern": "^[1-9][0-9]*$"
            },
            "name": {
                "id": "/properties/name",
                "type": "string"
            },
            "price": {
                "id": "/properties/price",
                "type": "number"
            },
            "tags": {
                "id": "/properties/tags",
                "items": {
                    "id": "/properties/tags/items",
                    "type": "string"
                },
                "type": "array"
            }
        },
        "required": ["id", "name"],
        "type": "object"
        
    }

